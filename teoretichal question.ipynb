   "source": [
    "a = [2,8,4,16,9,8]\n",
    "l = 0\n",
    "n = len(a)\n",
    "def swapList(a, l, n):\n",
    "    for i in range(n//2):\n",
    "        tmp = a[i]\n",
    "        a[i] = a[n-1-l]\n",
    "        a[n-1-l] = tmp\n",
    "        l += 1\n",
    "    return a\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 10,
   "metadata": {},
